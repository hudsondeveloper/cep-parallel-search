{
  "name": "cep-parallel-search",
  "version": "1.0.2",
  "description": "Biblioteca Node.js para busca de CEP com múltiplas consultas em paralelo e tratamento robusto de erros, com cache que acelera a busca em até 5x",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "babel src --out-dir dist --presets=@babel/preset-env --plugins=@babel/plugin-transform-modules-commonjs",
    "build:esm": "babel src --out-dir dist --presets=@babel/preset-env --keep-file-extension",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "cep",
    "zipcode",
    "brazil",
    "correios",
    "viacep",
    "brasilapi",
    "parallel",
    "async",
    "promise",
    "address"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/plugin-transform-modules-commonjs": "^7.23.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": ""
}
